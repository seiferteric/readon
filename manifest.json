{
  "manifest_version": 2,
  "name": "Readon!",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["gutindex.js"]
  },
  "browser_action": {
    "default_icon": "readon.png",
    "default_popup": "popup.html"
  }
}
